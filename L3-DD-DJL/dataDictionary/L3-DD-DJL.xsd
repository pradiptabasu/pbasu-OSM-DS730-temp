                    </xs:complexContent>
                  </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="ShipOrderFunction">
                  <xs:complexType>
                    <xs:complexContent>
                      <xs:extension base="tns:FunctionType"/>
                    </xs:complexContent>
